{
    "supportsEmptyDataView": true,
    "privileges": [
        {
            "name": "ExportContent",
            "essential": false
        }
    ],
    "dataRoles": [
        {
            "displayName": "Start Date",
            "name": "startDate",
            "kind": "Grouping",
            "description": "The column corresponding to your start date"
        },
        {
            "displayName": "End Date",
            "name": "endDate",
            "kind": "Grouping",
            "description": "The column corresponding to your end date"
        },
        {
            "displayName": "Event Label",
            "name": "label",
            "kind": "Grouping",
            "description": "The label you want to display on each event"
        },
        {
            "displayName": "Grouping 1",
            "name": "group1",
            "kind": "Grouping",
            "description": "Innermost column to group by (optional)"
        },
        {
            "displayName": "Grouping 2",
            "name": "group2",
            "kind": "Grouping",
            "description": "2nd column to group by (optional)"
        },
        {
            "displayName": "Grouping 3",
            "name": "group3",
            "kind": "Grouping",
            "description": "3rd column to group by (optional)"
        },
        {
            "displayName": "Grouping 4",
            "name": "group4",
            "kind": "Grouping",
            "description": "4th column to group by (optional)"
        },
        {
            "displayName": "Grouping 5",
            "name": "group5",
            "kind": "Grouping",
            "description": "5th column to group by (optional)"
        },
        {
            "displayName": "Grouping 6",
            "name": "group6",
            "kind": "Grouping",
            "description": "6th column to group by (optional)"
        },
        {
            "name": "tooltip",
            "displayName": "Description",
            "displayNameKey": "Role_Tooltips",
            "kind": "Grouping",
            "description": "Add in an extra column for description to appear in tooltip"
        }
    ],
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "startDate": {
                        "max": 1
                    },
                    "endDate": {
                        "max": 1
                    },
                    "label":{
                        "max": 1
                    },
                    "group1": {
                        "max": 1
                    },
                    "group2": {
                        "max": 1
                    },
                    "group3": {
                        "max": 1
                    },
                    "group4": {
                        "max": 1
                    },
                    "group5": {
                        "max": 1
                    },
                    "group6": {
                        "max": 1
                    },
                    "tooltip": {
                        "max": 1
                    }
                }
            ],
            "table": {
                "rows": {
                    "select": [
                        {
                            "for": {
                                "in": "group1"
                            }
                        },
                        {
                            "for": {
                                "in": "group2"
                            }
                        },
                        {
                            "for": {
                                "in": "group3"
                            }
                        },
                        {
                            "for": {
                                "in": "group4"
                            }
                        },
                        {
                            "for": {
                                "in": "group5"
                            }
                        },
                        {
                            "for": {
                                "in": "group6"
                            }
                        },
                        {
                            "for": {
                                "in": "tooltip"
                            }
                        },
                        {
                            "for": {
                                "in": "startDate"
                            }
                        },
                        {
                            "for": {
                                "in": "label"
                            }
                        },
                        {
                            "for": {
                                "in": "endDate"
                            }
                        }
                    ]
                }
            }
        }
    ],
    "objects": {
        "event": {
            "properties": {
                "eventFontColor": {
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "fontColorOverride": {
                    "type": {
                        "bool": true
                    }
                },
                "eventFontSize": {
                    "type": {
                        "numeric": true
                    }
                },
                "eventTextRotation": {
                    "type": {
                        "numeric": true
                    }
                },
                "waterfall": {
                    "type": {
                        "bool": true
                    }
                }
            }
        },
        "category": {
            "properties": {
                "categoryFontSize": {
                    "type": {
                        "numeric": true
                    }
                },
                "categoryTextRotation": {
                    "type": {
                        "numeric": true
                    }
                },
                "categoryGrayScale":{
                    "type": {
                        "bool": true
                    }
                }
            }
        }
    }
}